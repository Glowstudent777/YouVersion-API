{
  "name": "@glowstudent/youversion",
  "version": "1.0.0",
  "description": "Express Rest API for getting verses and such from YouVersion. ",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && npx tsc && npm run copyfiles",
    "clean": "npx rimraf --glob dist/",
    "copyfiles": "cp ./src/api/v1/core/db/ ./dist/api/v1/core/db/ -r",
    "dev": "npm run copyfiles && tsc-watch --onSuccess \"node ./dist/index.js\"",
    "start": "node ./dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Glowstudent777/YouVersion-API.git"
  },
  "keywords": [],
  "author": "Glowstudent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glowstudent777/YouVersion-API/issues"
  },
  "homepage": "https://github.com/Glowstudent777/YouVersion-API#readme",
  "dependencies": {
    "axios": "1.1.3",
    "body-parser": "^1.20.1",
    "cheerio": "1.0.0-rc.12",
    "cheerio-select": "2.1.0",
    "dotenv-extended": "^2.9.0",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "ioredis": "^5.4.2",
    "path": "^0.12.7",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^18.13.0",
    "@types/redis": "^4.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "rimraf": "^6.0.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5",
    "vitest": "^1.6.0"
  }
}
